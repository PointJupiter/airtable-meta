{
  "name": "airtable",
  "private": true,
  "version": "0.0.0",
  "description": "Airtable API client for TypeScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint {src,test}/**/*.ts",
    "build": "tsc",
    "docs": "typedoc",
    "prepublish": "rimraf lib && tsc"
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test",
      "pre-push": "yarn test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://git.pointjupiter.co/point-jupiter/tce/airtable-api.git"
  },
  "keywords": [
    "airtable",
    "api",
    "client",
    "metadata",
    "data"
  ],
  "author": "Point Jupiter <developers@pointjupiter.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://git.pointjupiter.co/point-jupiter/tce/airtable-api/issues"
  },
  "homepage": "https://git.pointjupiter.co/point-jupiter/tce/airtable-api#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.3"
  }
}
